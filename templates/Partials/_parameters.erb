<% parameters.each do |sp_parameter| -%>
<% sp_parameter.each do  |sp_var, data| -%>
    /// Specification details for <%= data["ak-variable"] %>
    public static let <%= data["ak-variable"] %>Def = NodeParameterDef(
        identifier: "<%= data["ak-variable"] %>",
        name: "<%= data["au-name"] %>",
        address: akGetParameterAddress("<%= node %>Parameter<%= data["ak-variable"].camelcase %>"),
        initialValue: <%= data["default"] %>,
        range: <%= data["minimum"] %> ... <%= data["maximum"] %>,
        unit: .<%= firstLower(data["au-unit"]) %>,
        flags: .default)

    /// <%= data["comment"] %>
    @Parameter(<%= data["ak-variable"] %>Def) public var <%= data["ak-variable"] %>: AUValue
<% end %>
<% end -%>
