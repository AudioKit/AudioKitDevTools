///
/// - parameter input: Input node to process
<% parameters.each do |parameter| -%>
<% if parameter[:type] == "Percent" -%>
    /// - parameter <%= parameter[:variable] %>: <%= parameter[:variable].underscore.humanize.titlecase %> (Normalized Value) ranges from 0 to 1 (Default: <%= parameter[:default].to_f/100 %>)
<% else -%>
    /// - parameter <%= parameter[:variable] %>: <%= parameter[:variable].underscore.humanize.titlecase %> (<%= parameter[:type] %>) ranges from <%= parameter[:min] %> to <%= parameter[:max] %> (Default: <%= parameter[:default] %>)
<% end -%>
<% end -%>
///